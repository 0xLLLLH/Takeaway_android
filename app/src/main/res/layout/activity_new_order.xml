<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.xllllh.android.takeaway.NewOrderActivity">

    <com.xllllh.android.takeaway.TopBar
        android:id="@+id/topbar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/topbar_height"
        app:titleIcon="@mipmap/arrow_left"
        app:titleText="提交订单"
        app:titleTextSize="@dimen/topbar_title_text_size"
        app:titleBackground="@null"
        app:showMenu="false"
        app:textColor="@color/colorTextDefault"
        android:background="@android:color/white"
        android:elevation="2dp">
    </com.xllllh.android.takeaway.TopBar>


</LinearLayout>
